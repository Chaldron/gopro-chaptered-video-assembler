    "version": "0.2.0",
    "configurations": [

        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'gopro-chaptered-video-assembler'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=gopro-chaptered-video-assembler",
                    "--package=gopro-chaptered-video-assembler"
                ],
                "filter": {
                    "name": "gopro-chaptered-video-assembler",
                    "kind": "bin"
                }
            },
            "args": ["-i", "test_data", "-o", "output"],
            "cwd": "${workspaceFolder}",
            "env": {
                "RUST_BACKTRACE": "1"
            }
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'gopro-chaptered-video-assembler'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=gopro-chaptered-video-assembler",
                    "--package=gopro-chaptered-video-assembler"
                ],
                "filter": {
                    "name": "gopro-chaptered-video-assembler",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}